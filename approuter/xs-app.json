{
  "authenticationMethod": "route",
  "welcomeFile": "/odata/v4/LibraryService/",
  "routes": [
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$",
      "target": "/$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    }
  ]
}
